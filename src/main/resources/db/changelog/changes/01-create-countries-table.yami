databaseChangeLog:
  -changeSet:
    id: 01-create-countries-table
    author: SukhachovS
    changes:
      -createTable:
        tableName: countries
        columns:
        -column:
          name: id
          type: bigint
          autoincrement: true
          constraints:
            primaryKey: true
            nullable: false
        -column:
          name: name
          type: varchar(255)
